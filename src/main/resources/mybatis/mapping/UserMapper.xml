<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.smallmall.dao.UserMapper">
	<resultMap type="com.smallmall.model.User" id="BaseResultMap">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="user_name" property="userName" jdbcType="VARCHAR" />
	</resultMap>

    <select id="getUserList" resultType="Map" parameterType="Map">
        SELECT id,user_name AS userName FROM com_user
    </select>
	

</mapper>